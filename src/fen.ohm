FEN {

Position = piecePlacement turn castling enPassantTarget plyClock moveNumber

piecePlacement = piecePlacementRank ("/" piecePlacementRank)*
piecePlacementRank = (digit | piece)*

turn = white | black

castling
  = letter+
  | empty

enPassantTarget
  = square
  | empty

white = "w"
black = "b"

piece = letter

square = file rank
file = "a".."h" | "A".."H"
rank = "1".."8"

plyClock = digit*
moveNumber = digit*

empty = "-"

}
